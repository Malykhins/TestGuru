<h1><%= t(".tests") %></h1>

<%= render 'shared/errors', resource: @test %>
<table class="table">
  <tr>
    <th class="sort-by-title pointer">
      <%= t('.title') %>
      <%= octicon 'arrow-up', class: 'text-success hide' %>
      <%= octicon 'arrow-down', class: 'text-success hide' %>
    </th>
    <th><%= t(".level") %></th>
    <th><%= t(".actions") %></th>
  </tr>

  <%= render @tests %>
</table>

<%= button_to "Create new test", new_admin_test_path, method: :get,  class: "btn btn-primary" %>
